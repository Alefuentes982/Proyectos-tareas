<h1>Proyectos</h1>
<%= link_to 'Nuevo proyecto', new_project_path %>
<ul>
  <% @projects.each do |project| %>
    <li>
      <%= link_to project.title, project %>
      <%= link_to 'Editar', edit_project_path(project) %>
      <%= link_to 'Eliminar', project, method: :delete, data: { confirm: 'Esta seguro que quiere eliminar el proyecto?' } %>
    </li>
  <% end %>
</ul>